OUTPUT_FORMAT("binary")
OUTPUT_ARCH(m68k)

MEMORY {
  ramsuper : ORIGIN = 0x10000, LENGTH =  0x10000 
}

SECTIONS
{
        /* le code du noyau de E2S */

        .text  0x10000 :
        {
                *(.text)
        } > ramsuper

        .data :
        {
                *(.data)
        } > ramsuper
       
	.bss :
	{
		*(.bss)
		*(COMMON)
	} > ramsuper
	
	_end = .;

} 

