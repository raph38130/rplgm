OUTPUT_FORMAT("binary")
OUTPUT_ARCH(m68k)

MEMORY {
  ramuser : ORIGIN = 0x80000000, LENGTH = 0x1000
}

SECTIONS
{
        /* le code qui s'execute en mode U */

        .text  0x80000004 :
        {
                *(.text)
        } > ramuser 

        .data :
        {
                *(.data)
        } > ramuser 

	.bss :
	{
		*(.bss)
		*(COMMON)
	} > ramuser 
	
	.user  0x80001000 :
	{
	LONG(0x12345678); /*juste pour forcer le remplissage jusquà cette adresse */
	___init = . ;
	} 


}

